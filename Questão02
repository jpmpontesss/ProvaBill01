#include <stdio.h>

int main()
{
    int x, y, auxiliar, i, n;

    x = 0;
    y = 1;

    printf("Digite um número:");
    scanf("%d", &n);
    printf("Série de Fibonacci:");
    printf("%d", y);

    for(i = 1; i < n; i++) {

        auxiliar = x + y;
        x = y;
        y = auxiliar;

        printf("%d", auxiliar);
    }
    
    return 0;
}
